{
  "name": "no-redux",
  "version": "0.2.1",
  "description": "A react/redux library which automates all redux flows. No more action creators, reducers, thunks/sagas..., and immutability is guaranteed. Redux becomes invisible to you, hence the name no-redux.",
  "keywords": [
    "javascript",
    "react",
    "redux",
    "ramda",
    "immutable",
    "redux-cycles",
    "cyclejs",
    "http-requests",
    "reselect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ln613/no-redux.git"
  },
  "main": "dist",
  "scripts": {
    "start": "watch \"npm run build\" src",
    "build": "babel src --out-dir dist",
    "test": "jest"
  },
  "author": "Nan Li",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@cycle/http": "^14.7.0",
    "ramda": "^0.25.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-cycles": "^0.4.1"
  }
}
